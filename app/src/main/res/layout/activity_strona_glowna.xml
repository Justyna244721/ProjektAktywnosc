<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F5EF"
    tools:context=".StronaGlowna">

    <Button
        android:id="@+id/cel"
        android:layout_width="138dp"
        android:layout_height="138dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="45dp"
        android:layout_marginRight="45dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/cwiczenia1"
        android:insetLeft="17dp"
        android:insetTop="17dp"
        android:insetRight="17dp"
        android:insetBottom="17dp"
        android:text="Button"
        app:backgroundTint="#D41579"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/waga"
        app:layout_constraintTop_toBottomOf="@+id/sen"
        app:layout_constraintVertical_bias="0.75" />

    <Button
        android:id="@+id/sen"
        android:layout_width="138dp"
        android:layout_height="138dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="45dp"
        android:layout_marginRight="45dp"
        android:layout_marginBottom="180dp"
        android:background="@drawable/cwiczenia1"
        android:insetLeft="17dp"
        android:insetTop="17dp"
        android:insetRight="17dp"
        android:insetBottom="17dp"
        android:text="Button"
        app:backgroundTint="#52B5CD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/woda"
        app:layout_constraintTop_toBottomOf="@+id/silownia"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/bieganie"
        android:layout_width="138dp"
        android:layout_height="138dp"
        android:layout_marginStart="45dp"
        android:layout_marginLeft="45dp"
        android:layout_marginEnd="228dp"
        android:layout_marginRight="228dp"
        android:layout_marginBottom="348dp"
        android:background="@drawable/cwiczenia1"
        android:insetLeft="17dp"
        android:insetTop="17dp"
        android:insetRight="17dp"
        android:insetBottom="17dp"
        android:text="Button"
        app:backgroundTint="#96C93D"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/silownia"
        android:layout_width="138dp"
        android:layout_height="138dp"
        android:layout_marginEnd="45dp"
        android:layout_marginRight="45dp"
        android:layout_marginBottom="348dp"
        android:background="@drawable/cwiczenia1"
        android:insetLeft="17dp"
        android:insetTop="17dp"
        android:insetRight="17dp"
        android:insetBottom="17dp"
        android:text="Button"
        app:backgroundTint="#D1D3D4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/bieganie" />

    <Button
        android:id="@+id/woda"
        android:layout_width="138dp"
        android:layout_height="138dp"
        android:layout_marginStart="45dp"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="228dp"
        android:layout_marginRight="228dp"
        android:layout_marginBottom="184dp"
        android:background="@drawable/cwiczenia1"
        android:insetLeft="17dp"
        android:insetTop="17dp"
        android:insetRight="17dp"
        android:insetBottom="17dp"
        android:text="Button"
        app:backgroundTint="#2374BB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bieganie" />

    <Button
        android:id="@+id/waga"
        android:layout_width="138dp"
        android:layout_height="138dp"
        android:layout_marginStart="45dp"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="228dp"
        android:layout_marginRight="228dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/cwiczenia1"
        android:insetLeft="17dp"
        android:insetTop="17dp"
        android:insetRight="17dp"
        android:insetBottom="17dp"
        android:text="Button"
        app:backgroundTint="#872A90"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/woda" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="231dp"
        android:layout_height="239dp"
        android:layout_marginTop="6dp"
        app:layout_constraintBottom_toTopOf="@+id/bieganie"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo1" />

</androidx.constraintlayout.widget.ConstraintLayout>